<center>
<h1>Events Page</h1>

<table>
  <% @events.each do |event| %>
  <tr>
	<td align='center'>
	  <%= link_to event.name, event %><br>
	  <%= event.location %><br>
	  <%= event.time %><br>
	  <br>
	</td>
  </tr>
  <% end %>
</table>

<br>
<h2>
  <%= link_to 'Add An Event', new_event_path %>
</h2>
<h2>
  <%= link_to 'Scrape for Events', scrape_path %>
</h2>
</center>
