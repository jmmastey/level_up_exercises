#!/usr/bin/env ruby

require_relative '../lib/data_science/conversion_test'
require_relative '../lib/data_science/json_parser'

conversion_test = DataScience::ConversionTest.new

data_file = ARGV.shift || 'source_data.json'
default_source_file = File.join( 'lib', 'data_science', data_file)

json_data = DataScience::JsonParser.parse_file(default_source_file)

conversion_test.import_data(json_data)

puts conversion_test.print_statistical_results("A", "B")
