<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_infor">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
  <div class="col-md-8">
    <h3>Weather</h3>
    <ol class="weather">
      <%= render  "weather" %>
    </ol>
  </div>
</div>

