            = haml :bomb_booted
          .bomb-activated
            = haml :bomb_activated
          .bomb-deactivated
            = haml :bomb_deactivated
          .bomb-exploded

  %script{src: '//code.jquery.com/jquery-1.11.3.min.js'}
  %script{src: "javascript/bomb.js", type: "text/javascript"}
