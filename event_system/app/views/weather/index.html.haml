%h1 Weather#index

= link_to('Register', register_index_path)

%form{:name => "city", :id => "city", :action => "/weather/index", :method => "get", :enctype => "text/plain"}
  %fieldset
    %li
      %input{:type => "text", :name => "region[name]", :class => "text"}
    %input{:type => "submit", :value => "Send", :class => "button"}

%table
  -if WeatherHelper.parse_temperatures != nil && params["region"]
    - @scraping.each do |key,hash_temp|
      %tr
        %td= key
        %td= hash_temp["low"].to_s  if hash_temp.has_key? "low"
        %td= hash_temp["high"].to_s if hash_temp.has_key? "high"

