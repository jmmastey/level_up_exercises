<%- model_class = Event -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<!--<table class="table table-striped">-->
  <!--<thead>-->
    <!--<tr>-->
      <!--<th><%= model_class.human_attribute_name(:id) %></th>-->
      <!--<th><%= model_class.human_attribute_name(:name) %></th>-->
      <!--<th><%= model_class.human_attribute_name(:venue_id) %></th>-->
      <!--<th><%= model_class.human_attribute_name(:description) %></th>-->
      <!--<th><%= model_class.human_attribute_name(:price) %></th>-->
      <!--<th><%= model_class.human_attribute_name(:show_type) %></th>-->
      <!--<th><%= model_class.human_attribute_name(:phone_number) %></th>-->
      <!--<th><%= model_class.human_attribute_name(:running_time) %></th>-->
      <!--<th><%= model_class.human_attribute_name(:event_url) %></th>-->
      <!--<th><%= model_class.human_attribute_name(:ticket_url) %></th>-->
      <!--<th><%= model_class.human_attribute_name(:created_at) %></th>-->
      <!--<th><%=t '.actions', :default => t("helpers.actions") %></th>-->
    <!--</tr>-->
  <!--</thead>-->
  <!--<tbody>-->
    <% @events.each do |event| %>
      <tr>
        <td><%= link_to event.id, event_path(event) %></td>
        <td><%= event.name %></td>
        <td><%= event.venue_id %></td>
        <td><%= event.description %></td>
        <td><%= event.price %></td>
        <td><%= event.show_type %></td>
        <td><%= event.phone_number %></td>
        <td><%= event.running_time %></td>
        <td><%= event.event_url %></td>
        <td><%= event.ticket_url %></td>
        <td><%=l event.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_event_path(event), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      event_path(event),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_event_path,
            :class => 'btn btn-primary' %>
